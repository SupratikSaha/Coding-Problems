   "source": [
    "# Using Bit Manipualation - O(1) run time, O(1) space"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def reverseBits(self, n: int) -> int:\n",
    "        ans = 0\n",
    "        for i in range(32):\n",
    "            ans = (ans << 1) + (n & 1)\n",
    "            n >>= 1\n",
    "        return ans"
   ]
  }
